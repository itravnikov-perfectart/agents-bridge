{
  "name": "agents-bridge",
  "repository": "https://github.com/agents-bridge/agents-bridge",
  "version": "0.0.1",
  "description": "Agents Bridge - VS Code extension, WebSocket server, and UI monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -r run dev",
    "lint": "pnpm -r run lint",
    "clean": "pnpm -r run clean",
    "type-check": "pnpm -r run type-check",
    "extension:build": "pnpm --filter agents-bridge-extension run build",
    "server:dev": "pnpm --filter agents-bridge-server run dev",
    "ui:dev": "pnpm --filter agents-bridge-ui run dev",
    "ui:build": "pnpm --filter agents-bridge-ui run build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-prettier": "^5.5.3",
    "knip": "^5.63.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
